                    <% houses.forEach((house, hi) => { %>
                      
                    <div class="col-md-4 simpleCart_shelfItem">
                      <div class="card mb-4 shadow-sm">
                    
                        <div id="carousel<%= house.id %>" class="carousel slide" data-ride="carousel">
                          <div class="carousel-inner">
                            <% var watch = 1; %>
                            <%  pictures.forEach((picture, pi) => { %>
                              
                            <%  if (picture.id == house.id) { %>
                              
                            <div class="carousel-item <%= (1 == watch ? 'active' : '' ) %>"> <!-- do for each change in hi -->
                              <img class="d-block w-100 <%= (1 == watch ? 'item_thumb' : '' ) %>" src="<%= picture.file %>" alt=" slide">
                            </div>
                            <% watch++; %>
                            <% } %>
                            <% }); %>
                          </div>
                          <a class="carousel-control-prev bg-dark" href="#carousel<%= house.id %>" role="button" data-slide="prev">
                            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                            <span class="sr-only">Previous</span>
                          </a>
                          <a class="carousel-control-next bg-dark" href="#carousel<%= house.id %>" role="button" data-slide="next">
                            <span class="carousel-control-next-icon" aria-hidden="true"></span>
                            <span class="sr-only">Next</span>
                          </a>
                        </div>
                    
                        <div class="card-body">
                          <div class="item_owner" hidden>Byarent</div>
                          <p class="lead item_name"><%= house.item_name %></p>
                          <p class="card-text"><%= house.description %>.</p>
                          <div class="d-flex justify-content-between align-items-center">
                            <div class="btn-group">
                              <a class="btn btn-sm btn-outline-secondary" href="/house?id=<%= house.id %>">View</a>
                              <!-- <button type="button" class="btn btn-sm btn-outline-secondary">View</button> -->
                              <button type="button" class="btn btn-sm btn-outline-secondary item_add">Add to cart</button>
                            </div>
                            <small class="text-muted item_price">&#8373; <%= house.price %></small>
                          </div>
                        </div>
                      </div>
                    </div>
                    
                    <% }); %>
        
                   
